<!DOCTYPE html>
<html lang="en">

<head>
    <title>Medical Details</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv='cache-control' content='no-cache'>
    <meta http-equiv='expires' content='0'>
    <meta http-equiv='pragma' content='no-cache'>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">
    {{!-- styles --}}
    {{>styles}}

</head>

<body>
    <div class="main-body">
        {{!-- navbar --}}
        {{>navbar}}

        <div class="container-contact100" style="padding: 70px; ">

            <div class="contact100-map" id="google_map" data-map-x="40.722047" data-map-y="-73.986422"
                data-pin="images/icons/map-marker.png" data-scrollwhell="0" data-draggable="1"></div>
            <div class="bg-video">
                <video class="bg-video__content" autoplay muted loop>
                    <source src="../static/img/video.mp4" type="video/mp4">
                    <source src="../static/img/video.webm" type="video/webm">
                    Your browser is not supported!
                </video>
            </div>

            <div class="wrap-contact100">

                <form class="contact100-form validate-form" method="POST" action="processsing">
                    <span class="contact100-form-title">
                        Medical Details
                    </span>

                    <div class="form-row p-t-20">
                        <div>
                            <label class="radio-container m-r-55">Update
                                <input type="radio" checked="checked" name="exist" value="exist1">
                                <span class="checkmark"></span>
                            </label>
                            <label class="radio-container">New
                                <input type="radio" name="exist" value="exist2">
                                <span class="checkmark"></span>
                            </label>
                        </div>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Please enter patient's uuid">
                        <input class="input100" type="number" name="uuid" placeholder="UUID">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Please enter doctor's duid">
                        <input class="input100" type="number" name="duid" placeholder="DUID">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Please enter patient's diabetesc level">
                        <input class="input100" type="number" name="diabetes_level" placeholder="Diabetes Level">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Please enter patient's bp level">
                        <input class="input100" type="number" name="bp_level" placeholder="Blood Pressure Level">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Please enter patient's diagnosis details">
                        <textarea class="input100" name="details" placeholder="Diagnosis Details"></textarea>
                        <span class="focus-input100"></span>
                    </div>

                    <div class="container-contact100-form-btn">
                        <button class="contact100-form-btn" type="submit" id="submit">
                            Register
                        </button>
                    </div>
                </form>

                <div class="contact100-more">
                    Contact us on: <span class="contact100-more-highlight">medfork@chennai.com</span>
                </div>
            </div>
        </div>



        <div id="dropDownSelect1"></div>

        {{!-- scripts --}}
        {{>scripts}}

        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag() { dataLayer.push(arguments); }
            gtag('js', new Date());

            gtag('config', 'UA-23581568-13');
        </script>

</body>

</html>