<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Doctor List</title>

    {{>dashboard-hospital-styles}}

    <style>
        thead {
            background-color: #333;
        }
    </style>


</head>


<body class="animsition">
    <div class="page-wrapper">

        <!-- MENU SIDEBAR-->
        {{>dashboard-hospital-sidebar}}
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            {{>dashboard-hospital-header}}

            <!-- END HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-md-12">
                                <!-- DATA TABLE -->
                                <h3 class="title-5 m-b-35">Doctor List</h3>
                                <div class="table-responsive table-responsive-data2">
                                    <table class="table table-data2">
                                        <thead>
                                            <tr>
                                                <th style="color: white;">S.No</th>
                                                <th style="color: white;">Name</th>
                                                <th style="color: white;">Reg No</th>
                                                <th style="color: white;">Reg Year</th>
                                                <th style="color: white;">State Medical Council</th>
                                                <th style="color: white;">Gender</th>
                                                <th style="color: white;">Contact</th>
                                                <th style="color: white;">status</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each doctorDetails}}
                                            <tr class="tr-shadow">
                                                <td id="nos"></td>
                                                <td>
                                                    <span class="block-email">{{this.doctorName}}</span>
                                                </td>
                                                <td class="desc">{{this.doctorRegNo}}</td>
                                                <td>{{this.doctorYearReg}}</td>
                                                <td>{{this.stateMedicalCouncil}}</td>
                                                <td>{{this.doctorGender}}</td>
                                                <td>{{this.doctorPnum}}</td>
                                                <td>
                                                    <span class="status--process">Active</span>
                                                </td>
                                            </tr>
                                            <tr class="spacer"></tr>
                                            {{/each}}

                                        </tbody>
                                    </table>
                                </div>
                                <!-- END DATA TABLE -->
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <p>Copyright Â© 2018 MedFork All rights reserved.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    {{>dashboard-hospital-scripts}}

    <script>
        let nos = 1;
        document.getElementById('nos').addEventListener('load', () => {
            document.getElementById('nos').innerHTML = nos++;
        });
    </script>

</body>

</html>
<!-- end document-->